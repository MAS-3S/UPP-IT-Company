        "\t\t\tchoosenTransport: $data.conferenceRequest.choosenTransport ? {",
        "\t\t\t\tpersistenceId_string: $data.conferenceRequest.choosenTransport.persistenceId_string !== undefined ? $data.conferenceRequest.choosenTransport.persistenceId_string : null,",
