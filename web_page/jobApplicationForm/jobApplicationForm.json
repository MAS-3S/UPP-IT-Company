      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.jobApplication && $data.jobApplication_potentialInterviewTerms && $data.jobApplication_choosenInterviewTerm && $data.jobApplication_interviewResults ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.jobApplication.potentialInterviewTerms = $data.jobApplication_potentialInterviewTerms;",
        "\t$data.jobApplication.choosenInterviewTerm = $data.jobApplication_choosenInterviewTerm;",
        "\t$data.jobApplication.interviewResults = $data.jobApplication_interviewResults;",
        "\treturn {",
        "\t\t//map jobApplication variable to expected task contract input",
        "\t\tjobApplicationInput: {",
        "\t\t\tname: $data.jobApplication.name !== undefined ? $data.jobApplication.name : null,",
        "\t\t\tsurname: $data.jobApplication.surname !== undefined ? $data.jobApplication.surname : null,",
